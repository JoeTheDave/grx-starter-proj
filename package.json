{
  "name": "grx-application-starter-kit",
  "version": "1.0.0",
  "author": "CyberGRX",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "start-dev-server": "scripts/start-dev-server.sh",
    "start-dev-client": "scripts/start-dev-client.sh",
    "start": "concurrently --kill-others \"npm:start-dev-server\" \"npm:start-dev-client\"",
    "compile-dev-server-debug": "scripts/compile-dev-server-debug.sh",
    "debug": "npm run compile-dev-server-debug && npm run start-dev-client",
    "build-server": "NODE_ENV=production webpack --config serverSrc/webpack.config.js",
    "build-client": "NODE_ENV=production webpack --config clientSrc/webpack.config.js",
    "prebuild": "npm run clean",
    "build": "npm run build-server & npm run build-client",
    "clean": "rimraf dist && rimraf build"
  },
  "dependencies": {
    "apollo-boost": "^0.1.14",
    "apollo-link-context": "^1.0.8",
    "apollo-server": "^2.0.0",
    "apollo-server-hapi": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "1.0.3",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-no-param-reassign-allow-reduce": "^1.0.1",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-whatwg-fetch": "^1.0.1",
    "flow-bin": "^0.75.0",
    "graphql": "^0.13.2",
    "hapi": "^17.5.2",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.3.0",
    "jss": "^9.8.6",
    "jss-camel-case": "^6.1.0",
    "jss-compose": "^5.0.0",
    "jss-default-unit": "^8.0.2",
    "jss-expand": "^5.3.0",
    "jss-nested": "^6.0.1",
    "jss-props-sort": "^6.0.0",
    "jss-vendor-prefixer": "^8.0.0",
    "lodash": "^4.17.10",
    "make-promises-safe": "^1.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.1",
    "react-jss": "^8.5.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "url-loader": "^1.0.1",
    "uuid": "^3.3.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-shell-plugin": "^0.5.0"
